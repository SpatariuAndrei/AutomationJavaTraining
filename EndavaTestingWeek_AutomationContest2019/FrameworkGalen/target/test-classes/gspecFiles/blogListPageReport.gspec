@objects
    header_menuOptions-*          xpath       //ul[@id='primary-menu']/li/a

    lambdaTest_blog               xpath       //h1[@class='blog-head-titel']
    message_Body                  xpath       //p[@class='blog-sub-titel']
    search_Bar                    xpath       //input[@type='search']
    social_MediaButtons-*         xpath       //div[@class='a2a_kit a2a_kit_size_26 addtoany_list']
    views_Comments_Buttons-*      xpath       //ul[@class='comm-count']
    post_title-*                  xpath       //h2[@class='blog-titel']
    blog_description-*            xpath       //h2[@class='blog-titel']/../p[2]
    pictures_blog-*               xpath       //div[@class='col-xs-12 col-md-5 blog-paddl0 thumb-img']
    categories_section            xpath       //h3[@class='widget-title']
    categories_optionsMenu-*      xpath       //div[@class='menu-side-menu-container']//a
    avatar_image-*                xpath       //div[@class='img-blog']/img

    footerElements-*              xpath       //ul[@class='list-unstyled list-inline ft-menu coll-box']/li/a
    footerElements_2-*            xpath       //ul[@class='list-inline list-unstyled social-links']/li/a
    footerElements_3-*            xpath       //footer[@class='footer']/div[@class='container']/div[@class='row']/div[2]//li/a

@script
    data = ["Recent Posts", "Infographics", "Product Updates", "Browser Testing", "Miscellaneous"];
    dataCategories = ["Automation","DevOps","Manual Testing","Mobile Testing","Performance","Cross Browser Testing"
                        ,"LambdaTest Experiments","JavaScript","Accessibility Testing","Microservice Architecture"
                        ,"Usability Testing","Regression Testing","Selenium Tutorial","Management","Web Development"];
    dataFooter = ["FAQ","Features","Team","List of Browsers","Career","Integrations","Contact Us"
                  ,"For Startups","Become an Affiliate","Support","Partners","LambdaTest Reviews"];

=BlogListPage header section=
    @on desktop,tablet
        @forEach [header_menuOptions-*] as itemName, index as i
           ${itemName}:
                height 50 to 60px
                width 100 to 250px
                count any forgotPassword_noAccount-* is < 6
                css box-sizing is "border-box"
                css line-height is "15px"
                css font-size is "15px"
                css padding is "22px 50px"
                text is "${data[i-1]}"

    @on mobile
        @forEach [header_menuOptions-*] as itemName, index as i
           ${itemName}:
               absent


=BlogListPage body section=
    @on desktop,tablet,mobile
        lambdaTest_blog:
                css font-size is "30px"
                css font-weight is "600"
                css letter-spacing is "2.5px"
                css color is "rgba(74, 74, 74, 1)"
                css margin-bottom is "0px"
                css margin-top is "15px"
                css text-align is "center"
                css text-transform is "uppercase"

        message_Body:
                css font-size is "16px"
                css font-weight is "400"
                css letter-spacing is "0.5px"
                css color is "rgba(74, 74, 74, 1)"
                css margin-bottom is "15px"
                css margin-top is "6px"
                css text-align is "center"
                css text-transform is "none"

        search_Bar:
                width is 420px
                height is 38px
                css border-radius is "19px"
                css background-color is "rgba(255, 255, 255, 1)"
                css border is "1px solid rgb(234, 234, 234)"
                css padding is "5px 25px"
                css font-size is "12px"
                css font-weight is "400"
                css background-position is "right 14px top 6px"


        @forEach [social_MediaButtons-*] as itemName, next as nextItem
           ${itemName}:
                height 17px
                width 100 to 250px
                count any social_MediaButtons-* is < 11
                css box-sizing is "border-box"
                css line-height is "26px"
                css background-color is "rgba(0, 0, 0, 0)"
                css font-size is "14px"
                css padding is "0px"
                css color is "rgba(20, 20, 18, 1)"
                css vertical-align is "baseline"
                right-of ${nextItem} -550 to -150px
                below ${nextItem} -600 to -300px
                near ${nextItem} 200 to 600px top, -400 to -110px left


        @forEach [views_Comments_Buttons-*] as itemName, next as nextItem
           ${itemName}:
                height 21px
                width 100 to 350px
                count any views_Comments_Buttons-* is < 11
                css box-sizing is "border-box"
                css line-height is "21px"
                css background-color is "rgba(0, 0, 0, 0)"
                css font-size is "14px"
                css padding is "0px"
                css color is "rgba(20, 20, 18, 1)"
                css vertical-align is "baseline"
                right-of ${nextItem} -550 to -210px
                below ${nextItem} -600 to -300px
                near ${nextItem} 200 to 600px top, -400 to -110px left

        @forEach [post_title-*] as itemName, next as nextItem
           ${itemName}:
                height 10px
                width 300 to 600px
                count any post_title-* is < 11
                css box-sizing is "border-box"
                css background-color is "rgba(0, 0, 0, 0)"
                css padding is "0px"
                css color is "rgba(74, 74, 74, 1)"
                css vertical-align is "baseline"
                below ${nextItem} -650 to -300px
                near ${nextItem} 200 to 600px top, -600 to -110px left


        @forEach [blog_description-*] as itemName, next as nextItem
           ${itemName}:
                height 50 to 130px
                width 300 to 600px
                count any blog_description-* is < 11
                css box-sizing is "border-box"
                css background-color is "rgba(0, 0, 0, 0)"
                css font-size is "16px"
                css padding is "0px"
                css color is "rgba(115, 115, 115, 1)"
                css vertical-align is "baseline"
                below ${nextItem} -800 to -300px
                near ${nextItem} 200 to 500px top, -600 to -110px left

        @forEach [pictures_blog-*] as itemName, next as nextItem
           ${itemName}:
                height 20 to 300px
                width 100 to 600px
                count any pictures_blog-* is < 11
                css box-sizing is "border-box"
                css line-height is "21px"
                css background-color is "rgba(0, 0, 0, 0)"
                css font-size is "14px"
                css color is "rgba(20, 20, 18, 1)"
                css vertical-align is "baseline"
                below ${nextItem} -800 to -300px
                near ${nextItem} 100 to 500px top, -600 to -110px left

        categories_section:
                visible
                css font-size is "16px"
                css font-weight is "900"
                css color is "rgba(14, 186, 197, 1)"
                css margin-top is "0px"
                css line-height is "34px"

        @forEach [categories_optionsMenu-*] as itemName, index as i
           ${itemName}:
                height 10 to 50px
                width 40 to 250px
                count any categories_optionsMenu-* is < 16
                css box-sizing is "border-box"
                css background-color is "rgba(0, 0, 0, 0)"
                css font-size is "14px"
                css padding is "0px"
                css color is "rgba(115, 115, 115, 1)"
                css vertical-align is "baseline"
                text is "${dataCategories[i-1]}"


=BlogListPage footer section=
    @on desktop,tablet,mobile
        @forEach [footerElements-*] as itemName, next as nextItem
           ${itemName}:
                height 10 to 100px
                width 40 to 250px
                count any footerElements-* is < 4
                css box-sizing is "border-box"
                css background-color is "rgba(0, 0, 0, 0)"
                css font-size is "14px"
                css color is "rgba(155, 155, 155, 1)"
                below ${nextItem} -500 to -10px
                near ${nextItem} -100 to 400px top, -500 to -110px left

        @forEach [footerElements_2-*] as itemName, next as nextItem
           ${itemName}:
                height 10 to 100px
                width 10 to 200px
                count any footerElements_2-* is < 5
                css box-sizing is "border-box"
                css background-color is "rgba(159, 162, 167, 1)"
                css font-size is "14px"
                css color is "rgba(255, 255, 255, 1)"
                below ${nextItem} -500 to -10px
                near ${nextItem} -200 to 400px top, -500 to 100px left

        @forEach [footerElements_3-*] as itemName, index as i
           @if ${isVisible(itemName)}
                ${itemName}:
                     visible
                     css font-size is "14px"
                     css background-color is "rgba(0, 0, 0, 0)"
                     css line-height is "21px"
                     css color is "rgba(155, 155, 155, 1)"
                     height 10 to 50px
                     width 40 to 250px
                     text is "${dataFooter[i-1]}"
                     css font-weight is "500"