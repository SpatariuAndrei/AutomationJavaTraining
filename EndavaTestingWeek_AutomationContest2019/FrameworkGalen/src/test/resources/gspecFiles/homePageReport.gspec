@objects
    lambda_header_logoD        xpath       //a[@class='navbar-brand']//img[1]
    lambda_header_logoM        xpath       //a[@class='navbar-brand']//img[2]
    menu_headerItem-*          xpath       //ul[@class='nav navbar-nav navbar-right']/li
    free_signUp                xpath       //li[@class='login']/a

    start_testing_button       xpath       //a[@class='home-cta']
    perform_message_center     xpath       //p[@class='pages-sub-titel']
    cross_message_center       xpath       //h1[@class='pages-titel']
    menu_bodyItem-*            xpath       //ul[@class='clint-logo-hover']/li/img

    rights_footer_message      xpath       //div[@class='col-sm-4']/span
    contactUs_footer_message   xpath       //ul[@class='list-unstyled list-inline ft-menu']//a[contains(text(),'Contact')]


=HomePage header section=
    @on desktop
        lambda_header_logoD:
            visible
            css vertical-align is "middle"
            css font-size is "18px"
            css background-color is "rgba(0, 0, 0, 0)"
            css line-height is "20px"
            css height is "30px"
            near cross_message_center 88px top, -160px left
            near perform_message_center 230 to 270px top, -160px left
            near start_testing_button 310 to 350px top, 187px left
            below cross_message_center -285 px
            below perform_message_center -322 px
            below start_testing_button -413 px

        @forEach [menu_headerItem-*] as itemName, index as i
            @if ${isVisible(itemName)}
                ${itemName}:
                    visible
                    css font-size is "14px"
                    css padding-right is "0px"
                    css padding-left is "0px"
                    css background-color is "rgba(0, 0, 0, 0)"
                    css line-height is "21px"
            @else
                @die "There are no menu items"

        free_signUp:
            visible
            css font-weight is "600"
            css font-size is "12px"
            css border-radius is "0px"
            css padding-left is "10px"
            css padding-right is "10px"
            css width is "120px"
            css text-transform is "uppercase"


    @on mobile
        lambda_header_logoD:
            visible
            css vertical-align is "middle"
            css font-size is "18px"
            css background-color is "rgba(0, 0, 0, 0)"
            css line-height is "20px"
            css height is "30px"
            near cross_message_center 88px top, -160px left
            near perform_message_center 230 to 270px top, -160px left
            near start_testing_button 310 to 350px top, 187px left
            below cross_message_center -285 px
            below perform_message_center -322 px
            below start_testing_button -413 px

        @forEach [menu_headerItem-*] as itemName, index as i
            @if ${isVisible(itemName)}
                ${itemName}:
                      visible
                      css font-size is "14px"
                      css padding-right is "0px"
                      css padding-left is "0px"
                      css background-color is "rgba(0, 0, 0, 0)"
                      css line-height is "21px"
            @else
                 ${itemName}:
                      absent

    @on tablet
        lambda_header_logoD:
           visible
           css vertical-align is "middle"
           css font-size is "18px"
           css background-color is "rgba(0, 0, 0, 0)"
           css line-height is "20px"
           css height is "30px"
           near cross_message_center 88px top, -160px left
           near perform_message_center 230 to 270px top, -160px left
           near start_testing_button 310 to 350px top, 187px left
           below cross_message_center -285 px
           below perform_message_center -322 px
           below start_testing_button -413 px

        @for [1 - 5,7] as index
           menu_headerItem-${index}:
               visible
               css font-size is "14px"
               css padding-right is "0px"
               css padding-left is "0px"
               css background-color is "rgba(0, 0, 0, 0)"
               css line-height is "21px"
               css line-height is "21px"

        free_signUp:
           visible
           css font-weight is "600"
           css font-size is "12px"
           css border-radius is "0px"
           css padding-left is "10px"
           css padding-right is "10px"
           css width is "120px"
           css text-transform is "uppercase"


=HomePage body section=
    @on desktop,tablet
        start_testing_button:
            css font-size is "14px"
            css background-color is "rgba(0, 0, 0, 1)"
            css margin-right is "20px"
            css text-transform is "uppercase"
            css font-weight is "400"
            css font-size is "14px"
            css letter-spacing is "0.9px"
            css text-align is "center"

        perform_message_center:
            css background-color is "rgba(0, 0, 0, 0)"
            css font-size is "18px"
            css letter-spacing is "0.4px"
            css margin-bottom is "10px"
            css text-align is "center"

        cross_message_center:
            css font-size is "64px"
            css font-weight is "300"
            css letter-spacing is "4.3px"
            css text-align is "center"
            css background-color is "rgba(0, 0, 0, 0)"
            css text-transform is "uppercase"
            css line-height is "83.2px"
            css margin-top is "77.7px"
            text contains "CROSS"

        @forEach [menu_bodyItem-*] as itemName, index as i
            @if ${isVisible(itemName)}
                ${itemName}:
                    visible
                    css margin-left is "0px"
                    width is 150 to 170px
                    css opacity is "0.7"
                    css display is "block"
                    css filter is "grayscale(1)"
                    css vertical-align is "middle"

    @on mobile
        start_testing_button:
            css font-size is "14px"
            css background-color is "rgba(0, 0, 0, 1)"
            css margin-right is "0px"
            css text-transform is "uppercase"
            css font-weight is "400"
            css font-size is "14px"
            css letter-spacing is "0.9px"
            css text-align is "center"

        perform_message_center:
            css background-color is "rgba(0, 0, 0, 0)"
            css font-size is "18px"
            css letter-spacing is "0.4px"
            css margin-bottom is "10px"
            css text-align is "center"

        cross_message_center:
            css font-size is "30px"
            css font-weight is "300"
            css letter-spacing is "4.3px"
            css text-align is "center"
            css background-color is "rgba(0, 0, 0, 0)"
            css text-transform is "uppercase"
            css line-height is "39px"
            css margin-top is "77.7px"

        @forEach [menu_bodyItem-*] as itemName, index as i
            @if ${isVisible(itemName)}
                ${itemName}:
                     visible
                     css margin-left is "0px"
                     width is 150 to 170px
                     css opacity is "0.7"
                     css display is "block"
                     css filter is "grayscale(1)"
                     css vertical-align is "middle"
            @else
                ${itemName}:
                     absent


=HomePage footer section=
    @on desktop,tablet,mobile
        rights_footer_message:
            visible
            css box-sizing is "border-box"
            css margin-top is "0px"
            css padding-top is "0px"
            css color is "rgba(161, 161, 161, 1)"

        contactUs_footer_message:
            visible
            css display is "block"
            css font-weight is "500"
            css color is "rgba(155, 155, 155, 1)"





