@objects
    lambda_header_logo             xpath       //a[@class='navbar-brand']//img[1]
    menu_headerItems-*             xpath       //ul[@class='navbar-nav ml-auto']/li/a
    username_header_DropDown       xpath       //li[@class='nav-item dropdown']/a/span[1]
    username_headerMenu_dropDown   xpath       //li[@class='nav-item dropdown show']/div/a

    verify_emailFullMessage        xpath       //div[@class='col-md-8']//div[contains(text(),'Verify')]
    enter_pinMessage               xpath       //div[@class='col-md-8']/div/div[2]
    pin_WebField                   xpath       //div[@class='col-md-6']
    verify_Button                  xpath       //button[@type='submit']
    resend_Pin                     xpath       //div[@class='col-md-8 offset-md-4']/a


=AccountPage header section=
    @on desktop, tablet
        lambda_header_logo:
            visible
            css vertical-align is "middle"
            css font-size is "16px"
            css background-color is "rgba(0, 0, 0, 0)"
            css line-height is "24px"
            css height is "27px"
            above verify_Button 230px
            near verify_Button 230px top, -200 to -110px left

        @forEach [menu_headerItems-*] as itemName, index as i
            @if ${isVisible(itemName)}
                ${itemName}:
                    visible
                    css color is "rgba(52, 58, 64, 1)"
                    css padding is "24px"
                    css font is "normal normal 400 normal 12.8px / 19.2px Lato, sans-serif"

        username_header_DropDown:
            visible
            css box-sizing is "border-box"
            css color is "rgba(0, 0, 0, 0.5)"
            css color is "rgba(0, 0, 0, 0.5)"
            css font is "normal normal 400 normal 12.8px / 19.2px Lato, sans-serif"
            above verify_Button 150 to 250px
            near verify_Button 200 to 250px top, -700 to -300px left


    @on mobile
        lambda_header_logo:
            visible
            css vertical-align is "middle"
            css font-size is "16px"
            css background-color is "rgba(0, 0, 0, 0)"
            css line-height is "24px"
            css height is "27px"
            above verify_Button 230px
            near verify_Button 230px top, -200 to -110px left



=AccountPage body section=
    @on desktop, tablet
        verify_emailFullMessage:
            visible
            text is "Verify Your Email Address"
            css color is "rgba(33, 37, 41, 1)"
            css font-size is "19.2px"
            css height is "52px"
            css font is "normal normal 400 normal 19.2px / 28.8px Lato, sans-serif"
            above resend_Pin 90 px
            near resend_Pin 100 to 150px top, -450 to -100px left

        enter_pinMessage:
            visible
            css padding is "20px"
            css box-sizing is "border-box"
            css display is "block"
            css background-color is "rgba(0, 0, 0, 0)"
            css background-clip is "border-box"
            css font-family is "Lato, sans-serif"
            css font-size is "12.8px"
            css font-weight is "400"
            css color is "rgba(33, 37, 41, 1)"
            css text-align is "left"

        pin_WebField:
            visible
            css position is "relative"
            width is 200 to 500px
            css padding-right is "15px"
            css padding-left is "15px"
            css box-sizing is "border-box"
            css font-size is "12.8px"
            css font-weight is "400"
            css line-height is "19.2px"

        verify_Button:
            visible
            css color is "rgba(255, 255, 255, 1)"
            css background-color is "rgba(14, 186, 197, 1)"
            css border-color is "rgb(14, 186, 197)"
            css font-weight is "400"
            css text-align is "center"
            css vertical-align is "middle"

        resend_Pin:
            visible
            css box-sizing is "border-box"
            css position is "static"
            css flex-direction is "row"
            css background-color is "rgba(0, 0, 0, 0)"
            css color is "rgba(14, 186, 197, 1)"
            css background-clip is "border-box"

    @on mobile
        verify_emailFullMessage:
            visible
            text contains "Verify"
            css color is "rgba(33, 37, 41, 1)"
            css font-size is "19.2px"
            css height is "52px"
            css font is "normal normal 400 normal 19.2px / 28.8px Lato, sans-serif"
            above resend_Pin 90px
            near resend_Pin 100 to 150px top, -450 to -100px left

        enter_pinMessage:
            visible
            css padding is "20px"
            css box-sizing is "border-box"
            css display is "block"
            css background-color is "rgba(0, 0, 0, 0)"
            css background-clip is "border-box"
            css font-family is "Lato, sans-serif"
            css font-size is "12.8px"
            css font-weight is "400"
            css color is "rgba(33, 37, 41, 1)"
            css text-align is "left"

        pin_WebField:
            visible
            css position is "relative"
            width is 200 to 500 px
            css padding-right is "15px"
            css padding-left is "15px"
            css box-sizing is "border-box"
            css font-size is "12.8px"
            css font-weight is "400"
            css line-height is "19.2px"

        verify_Button:
            visible
            css color is "rgba(255, 255, 255, 1)"
            css background-color is "rgba(14, 186, 197, 1)"
            css border-color is "rgb(14, 186, 197)"
            css font-weight is "400"
            css text-align is "center"
            css vertical-align is "middle"

        resend_Pin:
            visible
            css box-sizing is "border-box"
            css position is "static"
            css flex-direction is "row"
            css background-color is "rgba(0, 0, 0, 0)"
            css color is "rgba(14, 186, 197, 1)"
            css background-clip is "border-box"

