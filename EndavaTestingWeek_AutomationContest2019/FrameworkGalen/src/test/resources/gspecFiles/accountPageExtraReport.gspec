@objects
    username_headerMenu_dropDown-*   xpath       //li[@class='nav-item dropdown show']/div/a

=AccountPage header section=
    @on desktop,tablet,mobile
        @forEach [username_headerMenu_dropDown-*] as itemName, next as nextItem
            @if ${isVisible(itemName)}
                ${itemName}:
                    visible
                    css color is "rgba(33, 37, 41, 1)"
                    css padding is "4px 24px"
                    css font is "normal normal 400 normal 12.8px / 19.2px Lato, sans-serif"
                    count any forgotPassword_noAccount-* is < 7
                    below ${nextItem} -100 to -20px
                    css box-sizing is "border-box"
                    css line-height is "19.2px"
                    height 27px
                    width 100 to 600px
                    aligned vertically all ${nextItem}






